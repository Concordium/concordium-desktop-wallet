{
    "plugins": ["stylelint-scss"],
    "extends": ["stylelint-config-standard", "stylelint-config-prettier"],
    "rules": {
        "property-no-unknown": [
            true,
            {
                "ignoreProperties": ["composes"]
            }
        ],
        "at-rule-no-unknown": null,
        "scss/at-rule-no-unknown": true,
        "no-descending-specificity": null
    }
}
